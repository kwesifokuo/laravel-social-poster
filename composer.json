{
    "name": "cibs/laravel-social-poster",
    "description": "Laravel social auto posting (Larasap) lets you automatically post all your content to social networks such as Telegram Channel and Twitter.",
    "license": "MIT",
    "keywords": [
        "laravel",
        "social network",
        "telegram",
        "twitter",
        "facebook"
    ],
    "authors": [
        {
            "name": "Ernest O. Fokuo",
            "email": "kwesifokuo@gmail.com",
            "homepage": "https://chosenbiz.solutions",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "~7.0|^8.1.0",
        "janu-software/facebook-php-sdk": "^3.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "php-http/guzzle7-adapter": "1.x-dev"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.8"
    },
    "autoload": {
        "psr-4": {
            "Cibs\\LaravelSocialPoster\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Cibs\\LaravelSocialPoster\\LarasapServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
